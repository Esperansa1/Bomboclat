# CSGORoll Skin Sniper - required environment variables
# Copy this file to .env and fill in your credentials

# Full Cookie header value from browser DevTools (include ALL cookies: session, cf_clearance, __cf_bm, etc.)
# DevTools → Network → WS connection → Headers → Cookie (copy the entire value)
CSGOROLL_SESSION=session=s%3A...; cf_clearance=...; __cf_bm=...

# CapSolver API key (from https://dashboard.capsolver.com)
CAPSOLVER_API_KEY=your_capsolver_api_key_here
